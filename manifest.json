{
  "manifest_version": 2,

  "name": "Better Whatsapp Web",
  "description": "Improving the experience of Whatsapp web since 2016!",
  "version": "0.00000000000000000001",

  "browser_action": {
    "default_icon": "icon.png"
  },
    "content_scripts": [
    {
        "matches": ["https://web.whatsapp.com/*"],
        "css": ["whatsapp.css"],
        "js": ["jquery-3.1.1.min.js", "whatsapp.js"],
        "run_at": "document_idle" 
    }
  ]
}